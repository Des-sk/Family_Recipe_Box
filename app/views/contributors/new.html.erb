<h1> Signup </h1>
<%= render partial: 'layouts/errors',locals: {object: @contributor} %>

<%= form_for(@contributor) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email%>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign up" %>
<% end %>

<h3>Or Login with Google:</h3>

<%= button_to 'Google', '/auth/google_oauth2' %>
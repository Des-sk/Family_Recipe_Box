

<br>
<% if @recipe_creator %>
        <h1><%= @recipe_creator.name %>'s Recipe Box </h1> <br>
        <%= link_to "Add a New Recipe", new_recipe_creator_recipe_path %> <br>   
<% else %>
<h1> Recipe Box </h1>
<% end %>

        <% @recipes.each do |r| %><br>
            <h3><%= link_to r.title, recipe_path(r) %><h3>
            Created by: <%= r.recipe_creator.name %> <br>
            Added by: <%= r.contributor.name %> 
            <br>
            <br>
        <% end %> 
  

   

    
    
    

    
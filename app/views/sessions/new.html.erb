

<h1> Login </h1>

<%= form_with url: login_path do |f| %>
    <%= f.label :email%>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Login" %>
<% end %>

<h3>Or Login with Google:</h3>

<%= button_to 'Google', '/auth/google_oauth2' %>